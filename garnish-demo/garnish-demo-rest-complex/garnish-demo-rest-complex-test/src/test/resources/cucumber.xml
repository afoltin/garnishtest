<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <!--
        cucumber-spring integration will automatically load a "cucumber.xml" file from the root of the classpath

        For a good organization of spring xml files, we just import to our main file.
     -->

    <import resource="classpath:META-INF/spring/garnish-demo-rest-complex-test.xml" />

</beans>